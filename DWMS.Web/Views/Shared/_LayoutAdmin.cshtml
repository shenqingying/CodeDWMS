<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="~/lib/admin-lte/plugins/fontawesome-free/css/all.min.css">

    <!-- Theme style -->
    <link href="~/lib/admin-lte/css/adminlte.css" rel="stylesheet" />

    @* 增加 style 入口 *@
    @RenderSection("Styles", required: false)
</head>
<body class="sidebar-mini">
    <div class="wrapper">
        <!-- 1、头部标题栏 -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button">
                        <i class="fas fa-bars"></i>
                    </a>
                </li>
                @*<li class="nav-item d-none d-sm-inline-block">
                    <a class="nav-link" asp-controller="Home" asp-action="Index">Home</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a class="nav-link" asp-controller="Home" asp-action="Privacy">Privacy</a>
                </li>*@
            </ul>
        </nav>

        <!-- 2、侧边栏 -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="#" class="brand-link">
                <img src="~/lib/admin-lte/img/AdminLTELogo.png" alt="AdminLTE Logo"
                     class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light">DWMS</span>
            </a>
            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview"
                        role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class with font-awesome or any other icon font library -->
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-tachometer-alt"></i>
                                <p>
                                    驾驶舱
                                </p>
                            </a>
                        </li>

                        @* 数据仓库管理 *@
                        <li class="nav-item has-treeview menu">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-database"></i>
                                <p>
                                    数据仓库管理
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="~/dw" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>数据源</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="~/dw/DBPreview" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>数据表预览</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="~/dw/SONLUK" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Sonluk测试</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="~/dw/SQLModel" class="nav-link">
                                        <i class="fa fa-code nav-icon"></i>
                                        <p>SQL模型</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="~/dw/ValueRename" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>数据值映射</p>
                                    </a>
                                </li>
                            </ul>
                        </li>

                        @* 数据采集管理 *@
                        <li class="nav-item has-treeview menu">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-edit"></i>
                                <p>
                                    数据采集管理
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>采集模板管理</p>
                                    </a>
                                </li>
                            </ul>
                        </li>

                        @* 数据字典管理 *@
                        <li class="nav-item has-treeview menu">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-file"></i>
                                <p>
                                    数据字典管理
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>数据字典模板配置</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>TODO</p>
                                    </a>
                                </li>
                            </ul>
                        </li>

                        <li class="nav-item has-treeview menu">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-wrench"></i>
                                <p>
                                    系统管理
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="~/user" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>用户管理</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>TODO</p>
                                    </a>
                                </li>
                            </ul>
                        </li>

                        <li class="nav-item">
                            <a href="~/docs/roadmap" class="nav-link">
                                <i class="nav-icon fas fa-th"></i>
                                <p>
                                    开发路线图
                                    <span class="right badge badge-danger">New</span>
                                </p>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-th"></i>
                                <p>
                                    使用手册
                                    <span class="right badge badge-danger">New</span>
                                </p>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- 3、内容块 -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper" style="min-height: 371px;">
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        <!-- 4、底部标题栏 -->
        <footer class="main-footer">
            <!-- To the right -->
            <div class="float-right d-none d-sm-inline">
                探索更多 Explore More...
            </div>
            <!-- Default to the left -->
            <strong>Copyright &copy; @DateTime.Now.Year <a href="#">XX</a>.</strong> All rights reserved.
        </footer>
    </div>
    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/lib/admin-lte/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/lib/admin-lte/js/adminlte.min.js"></script>

    @* 增加 script 入口 *@
    @RenderSection("Scripts", required: false)
</body>
</html>
